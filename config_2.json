{
	"data": {
		"filename": "srikar and zeyang_clockwise 2.csv",
		"columns": [
			"Therapist position z",
			"Patient joint angle 0",
			"Patient joint angle 1",
			"Patient joint angle 2",
			"Patient joint angle 3",
			"Patient joint angle 4",
			"Patient joint angle 5",
			"Patient position x",
			"Patient position y",
			"Patient position z"
		],
		"sequence_length": 2,
		"train_test_split": 0.85,
		"normalise": false
	},
	"training": {
		"epochs": 80,
		"batch_size": 32
	},
	"model": {
		"loss": "mse",
		"optimizer": "adam",
		"save_dir": "saved_models",
		"layers": [
			{
				"type": "lstm",
				"neurons": 256,
				"input_timesteps": 1,
				"input_dim": 10,
				"return_seq": true
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "lstm",
				"neurons": 256,
				"return_seq": true
			},

			{
				"type": "lstm",
				"neurons": 256,
				"return_seq": false
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "dense",
				"neurons": 1,
				"activation": "linear"
			}
		]
	}
}